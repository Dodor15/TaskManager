<Application x:Class="TaskManager.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:TaskManager"
             StartupUri="MainWindow.xaml"
             xmlns:converters="clr-namespace:TaskManager.Converters">
    <Application.Resources>
        <ResourceDictionary>
            <converters:ButtonVisibilityConverter x:Key="buttonConverter"/>
            <converters:TextMissedDeadlineConverter x:Key="colorTime"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>
